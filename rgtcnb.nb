      SubscriptBox["Y", "bc"]}], "+", 
     RowBox[{
      SubscriptBox["X", "bd"], 
      SubscriptBox["Y", "ac"]}], "+", 
     RowBox[{
      FractionBox["1", 
       RowBox[{"n", "-", "2"}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SubscriptBox["g", "ac"], 
         SubscriptBox["Z", "bd"]}], "-", 
        RowBox[{
         SubscriptBox["g", "bc"], 
         SubscriptBox["Z", "ad"]}], "-", 
        RowBox[{
         SubscriptBox["g", "ad"], 
         SubscriptBox["Z", "bc"]}], "+", 
        RowBox[{
         SubscriptBox["g", "bd"], 
         SubscriptBox["Z", "ac"]}]}], ")"}]}], "+", 
     RowBox[{
      FractionBox["2", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"n", "-", "1"}], ")"}], 
        RowBox[{"(", 
         RowBox[{"n", "-", "2"}], ")"}]}]], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"X", " ", "Y"}], "-", 
        RowBox[{
         SubscriptBox[
          SuperscriptBox["X", "e"], "f"], 
         SubscriptBox[
          SuperscriptBox["Y", "f"], "e"]}]}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        RowBox[{
         SubscriptBox["g", "ac"], 
         SubscriptBox["g", "bd"]}], "-", 
        RowBox[{
         SubscriptBox["g", "bc"], 
         SubscriptBox["g", "ad"]}]}], ")"}]}]}]}], ","}]],ExpressionUUID->
  "751b8500-eed9-435e-b1c8-d013a96e6cfa"],
 "\nwhere ",
