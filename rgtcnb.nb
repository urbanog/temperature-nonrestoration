   RowBox[{
    TagBox[
     TagBox[
      SubscriptBox[
       SuperscriptBox["h", "s"], 
       RowBox[{",", "a"}]],
      DisplayForm],
     DisplayForm], 
    SubscriptBox["g", "sb"]}], "+", 
   RowBox[{
    TagBox[
     TagBox[
      SubscriptBox[
       SuperscriptBox["h", "s"], 
       RowBox[{",", "b"}]],
      DisplayForm],
     DisplayForm], 
    SubscriptBox["g", "as"]}]}]],ExpressionUUID->
  "7b681c09-e600-40bb-8632-e863f9a061f6"],
 "."
}], "Text",
 FontSize->16,ExpressionUUID->"843c0585-4e76-49a4-8417-ea862644797d"],

